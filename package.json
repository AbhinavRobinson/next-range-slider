    "devDependencies": {
        "@types/node": "^18.6.4",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.6.4"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
