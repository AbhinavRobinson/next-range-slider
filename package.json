{
    "name": "react-multirange-slider",
    "version": "1.0.0",
    "description": "A React component for multi-range slider",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "watch": "tsc --watch",
        "build": "rm -rf dist/ && prettier --write src/ && npm run build:cjs && npm run build:esm",
