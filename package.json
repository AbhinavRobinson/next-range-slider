{
  "name": "react-range-slider",
  "version": "1.0.0",
  "description": "A React component for range slider",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "npm-run-all watch:esm watch:cjs",
    "watch:esm": "tsc --watch",
    "watch:cjs": "tsc --watch --module CommonJS --outDir dist/cjs",
    "build": "rm -rf dist/ && prettier --write src/ && npm-run-all build:cjs build:esm",
    "build:esm": "tsc && cp src/components/*.css dist/esm/components",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs && cp src/components/*.css dist/cjs/components"
  },
  "author": {
    "name": "Abhinav Robinson",
    "email": "abhinavrobinson@gmail.com"
  },
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
